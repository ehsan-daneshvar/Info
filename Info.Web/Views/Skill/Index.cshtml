@model IEnumerable<Info.Domain.Entities.Skill>

<table class="table table-striped table-dark ">
    <thead>
    <tr>
        <th scope="col">Name</th>
        <th scope="col">Perecent</th>

    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Percent</td>

        </tr>
    }
    
    </tbody>
    
</table>
<div class="text-center"><button type="button" class="btn btn-primary w-100">Edit</button> </div>